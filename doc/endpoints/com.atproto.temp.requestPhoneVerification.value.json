{
  "post": {
    "tags": [
      "com.atproto.temp"
    ],
    "summary": "Request a verification code to be sent to the supplied phone number",
    "operationId": "com.atproto.temp.requestPhoneVerification",
    "requestBody": {
      "required": true,
      "content": {
        "application/json": {
          "schema": {
            "type": "object",
            "required": [
              "phoneNumber"
            ],
            "properties": {
              "phoneNumber": {
                "type": "string"
              }
            }
          }
        }
      }
    },
    "responses": {
      "200": {
        "description": "OK"
      },
      "400": {
        "description": "Bad Request",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "required": [
                "error",
                "message"
              ],
              "properties": {
                "error": {
                  "type": "string",
                  "oneOf": [
                    {
                      "const": "InvalidRequest"
                    }
                  ]
                },
                "message": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  }
}
